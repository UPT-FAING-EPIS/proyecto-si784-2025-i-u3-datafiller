{
  "name": "datafiller/project",
  "description": "DataFiller Project",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "fakerphp/faker": "^1.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "infection/infection": "^0.29.14"
  },
  "autoload": {
    "psr-4": {
      "App\\Controllers\\": "controllers/",
      "App\\Models\\":      "models/",
      "App\\Config\\":      "config/",
      "App\\Helpers\\":     "helpers/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
